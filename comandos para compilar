make
arm-uclinuxeabi-gcc -Os -mcpu=cortex-m3 -mthumb -I /home/arturo/FK/A2F/root/usr/include -c -o hello.o util.o hello.c util.c
arm-uclinuxeabi-gcc -mcpu=cortex-m3 -mthumb -L /home/arturo/FK/A2F/root/usr/include -o myexec hello.o util.o	

make
